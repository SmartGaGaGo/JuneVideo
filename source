local function EGETMVZ_fake_script() 
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Juned["_SAP"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end
	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer
	local HighlightEnabled = script.Parent.Parent.Parent.Parent.HighlightEnabled
	local showDistances = script.Parent.Parent.Parent.Parent.showDistances
	local TweenService = game:GetService("TweenService")
	local ESPEnabled = false
	local ESPFolder = Instance.new("Folder")
	ESPFolder.Name = "ESPFolder"
	ESPFolder.Parent = game.ReplicatedFirst
	local CLOSE_DISTANCE = 25
	local FAR_COLOR = Color3.new(0, 1, 0)
	local CLOSE_COLOR = Color3.new(1, 0, 0)
	local TWEEN_INFO = TweenInfo.new(0.1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut)
	local function removeESP()
		ESPFolder:ClearAllChildren()
	end
	local function createESP(player, character)
		if not ESPEnabled or player == LocalPlayer or not character then return end
		local hrp = character:WaitForChild("HumanoidRootPart", 5)
		local humanoid = character:WaitForChild("Humanoid", 5)
		if not hrp or not humanoid then return end
		ESPFolder:FindFirstChild(player.Name):Destroy()
		local billboard = Instance.new("BillboardGui")
		billboard.Name = player.Name
		billboard.Adornee = hrp
		billboard.Size = UDim2.new(0, 200, 0, 20)
		billboard.AlwaysOnTop = true
		billboard.Parent = ESPFolder
		local label = Instance.new("TextLabel")
		label.Size = UDim2.fromScale(1, 1)
		label.BackgroundTransparency = 1
		label.TextScaled = true
		label.Font = Enum.Font.RobotoMono
		label.TextStrokeTransparency = 0
		label.Parent = billboard
		local highlight = Instance.new("Highlight")
		highlight.Name = "ESPHighlight"
		highlight.Adornee = character
		highlight.FillColor = FAR_COLOR
		highlight.FillTransparency = 0
		highlight.OutlineTransparency = 1
		highlight.Enabled = HighlightEnabled.Value
		highlight.Parent = billboard
		task.spawn(function()
			while ESPEnabled and humanoid.Parent and character.Parent do
				if showDistances.Value and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
					local localHRP = LocalPlayer.Character.HumanoidRootPart
					local distance = (hrp.Position - localHRP.Position).Magnitude
					label.Text = string.format("%d", math.floor(distance + 0.5))
				else
					label.Text = player.DisplayName
				end
				label.TextColor3 = player.Team and player.Team.TeamColor.Color or Color3.new(1, 1, 1)
				local localHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
				if localHRP then
					local distance = (hrp.Position - localHRP.Position).Magnitude
					local targetColor = distance <= CLOSE_DISTANCE and CLOSE_COLOR or FAR_COLOR
					if highlight.FillColor ~= targetColor then
						TweenService:Create(highlight, TWEEN_INFO, { FillColor = targetColor }):Play()
					end
				end
				task.wait(0.15)
			end
		end)
	end
	local function trackPlayer(player)
		player.CharacterAdded:Connect(function(character)
			task.wait(0.1)
			createESP(player, character)
		end)
		if player.Character then
			createESP(player, player.Character)
		end
	end
	local function enableESP()
		if ESPEnabled then return end
		ESPEnabled = true
		for _, player in ipairs(Players:GetPlayers()) do
			trackPlayer(player)
		end
		Players.PlayerAdded:Connect(trackPlayer)
	end
	local function disableESP()
		ESPEnabled = false
		removeESP()
	end
	script.Parent.MouseButton1Click:Connect(function()
		ESPEnabled = not ESPEnabled
		if ESPEnabled then enableESP() else disableESP() end
	end)
end
local function JWRB_fake_script() 
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Juned["_DH"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end
	local ena = true
	local HighlightEnabled = script.Parent.Parent.Parent.Parent.HighlightEnabled
	local ESPFolder = game.ReplicatedFirst.ESPFolder
	script.Parent.MouseButton1Click:Connect(function()
		ena = not ena
		HighlightEnabled.Value = ena
		for _, obj in ipairs(ESPFolder:GetChildren()) do
			if obj:IsA("Highlight") then
				obj.Enabled = ena
			end
		end
	end)
end
local function UARERS_fake_script() 
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Juned["_DIoN"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end
	local showDistances = script.Parent.Parent.Parent.Parent.showDistances
	local ena = false
	script.Parent.MouseButton1Click:Connect(function()
		ena = not ena
		if ena then
			showDistances.Value = true
		else
			showDistances.Value = false
		end
	end)
end
local function MMURP_fake_script() 
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Juned["_JuneVideo"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end
	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer
	if LocalPlayer then
		LocalPlayer.Chatted:Connect(function(mes)
			if mes:lower() == "/e" or "/emote" or "/e open" then
				script.Parent.Main.Visible = true
				script.Parent.Main:TweenSize(
					UDim2.new(0, 250, 0.5, 0),
					Enum.EasingDirection.InOut,
					Enum.EasingStyle.Sine,
					.5
				)
			end
		end)
	end
end
